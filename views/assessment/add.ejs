<%- include('../layout/includes/header', { title: 'User Index Page' }) %>
    <div class="row fixed-div_">
        <nav aria-label="breadcrumb" class="breadcrumbs">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
                <li class="breadcrumb-item"><a href="#">Questionnaires</a></li>
                <li class="breadcrumb-item active" aria-current="page">Add Questionnaires </li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body p-0">
                    <form action="">
                        <div class="top_box___">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="title_head">
                                        <h4>Create Questionnaires</h4>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="mb-3 form-group">
                                        <label for="validationCustom01" class="form-label">Questionnaire Name</label>
                                        <input type="text" class="form-control" id="" placeholder="">
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="mb-3 form-group">
                                        <label class="form-label">Select Product</label>
                                        <select class="form-control select2_without_search">
                                            <option value="">Select</option>
                                            <option>Face mask</option>
                                            <option>Eye contour products</option>
                                            <option>Lip care products</option>
                                            <option>Body care products </option>
                                            <option>Soap product</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="question_box__">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">Question</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 1</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 2</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 3</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 4</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-12">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">Upload Terms &
                                                        policies Document</label>
                                                    <input name="file1" type="file" class="dropify" data-height="100" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-12" id="add_question">
                                    <div class="question_box__">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <div class="delete_icon text-end">
                                                    <a href="#" id="remove_question_box" data-bs-toggle="tooltip"
                                                        data-bs-placement="top" data-bs-title="Remove"><i
                                                            class="mdi mdi-delete"></i></a>
                                                </div>
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">Question</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 1</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 2</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 3</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-3">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">option 4</label>
                                                    <input type="text" class="form-control" id="" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-lg-12">
                                                <div class="mb-3 form-group">
                                                    <label for="validationCustom01" class="form-label">Upload Terms &
                                                        policies Document</label>
                                                    <input name="file1" type="file" class="dropify" data-height="100" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="add_more">
                                        <a href="#" id="add_more_btn">+ Add More</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="bottom__">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="text-end">
                                        <a href="/assessment"
                                            class="btn btn-primary waves-effect waves-light">Submit</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div> <!-- end col -->
    </div>
    <%- include('../layout/includes/footer') %>